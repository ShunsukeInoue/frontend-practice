curl -X POST "https://{resource-name}.openai.azure.com/openai/deployments/{deployment-id}/chat/completions?api-version=2024-10-21" \
  -H "Content-Type: application/json" \
  -H "api-key: {YOUR_AZURE_OPENAI_KEY}" \
  -d '{
    "messages":[{"role":"user","content":"hello"}],
    "temperature":0
  }'
